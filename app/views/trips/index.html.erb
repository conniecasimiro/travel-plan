<div class="body">
  <%= render "shared/navbar" %>
  <div class="container">
    <%# narbar section %>

    <%# trip picture and details %>
    <div class="trip-cards">
      <h1>Trips!</h1>
      <% @trips.each do |trip|%>
      <li><%= trip.title%></li>
      <li><%= trip.duration %></li>
      <li><%= trip.location %></li>
        </div>
      <%= link_to 'Details', trip_path(trip), class: "btn btn-primary"%>
      <%# <li><%= trip.photo %><%#/li>
      <%# trip price %>
      <%# trip tags %>
      <% end %>
      <%= cl_image_tag @landmarks.photo.key, height: 400, width: 400, class:"d-block w-100", style:"object-fit: cover" %>
    </div>
  </div>
  <%= render "shared/footer" %>
</div>
